{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["javascript", "javascriptreact"],
  "prettier.enableDebugLogs": false,
  "files.exclude": {
    "**/node_modules": true
  },
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false
}
